{
  "name": "miner-spy",
  "version": "1.0.0",
  "description": "Bittensor Subnet Miner Analysis Tool",
  "main": "server/index.js",
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
